{
  "name": "language-fudomo",
  "main": "./lib/language-fudomo",
  "version": "0.2.8",
  "description": "Language support for a model transformation approach based on functional decomposition",
  "keywords": [
    "model transformation",
    "code generation",
    "functional decomposition"
  ],
  "license": "MIT",
  "engines": {
    "atom": "1.37.0"
  },
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "fudomo-transform": "~0.1.7",
    "tree-sitter": "~0.15.5",
    "tree-sitter-fudomo": "~0.1.6",
    "vm2": "^3.8.2",
    "yaml": "^1.6.0"
  },
  "package-deps": [
    "linter:2.2.0"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "tree-view": {
      "versions": {
        "^1.0.0": "consumeTreeView"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lassy-fmde/language-fudomo.git"
  },
  "bugs": {
    "url": "https://github.com/lassy-fmde/language-fudomo/issues"
  },
  "homepage": "https://github.com/lassy-fmde/language-fudomo"
